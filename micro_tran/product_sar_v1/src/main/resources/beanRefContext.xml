<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="product-service-sar.context"
		class="org.springframework.context.support.GenericXmlApplicationContext">
		<constructor-arg>
			<array>
				<value>META-INF/context-export/context-applicationContext.xml</value>
				<value>META-INF/product-service/applicationContext-productService.xml</value>
			</array>
		</constructor-arg>
	</bean>
</beans>